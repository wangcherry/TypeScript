{
    "extends": [
        "tslint:recommended"
    ],
    "rules": {
        "no-console":false,
        "curly":false,
        "quotemark":false,
        "array-type":false,
        "no-unnecessary-initializer":false,
        "no-angle-bracket-type-assertion":false,
        "max-classes-per-file":false,
        "no-empty":false,
        "interface-over-type-literal":false
    }
}